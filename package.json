{
  "name": "trpc-azure-adapter",
  "version": "1.0.0",
  "scripts": {
    "build": "swc ./src -d dist/src",
    "watch": "swc ./src -d dist/src --watch --delete-dir-on-start",
    "start": "func start",
    "dev": "concurrently -n \"watch,func\" -c \"auto\" --kill-others \"npm:watch\" \"npm:start\""
  },
  "dependencies": {
    "@azure/functions": "^4.0.0",
    "@trpc/client": "^10.40.0",
    "@trpc/server": "^10.40.0",
    "cross-fetch": "^4.0.0",
    "superjson": "^1.13.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.92",
    "@types/node": "^18.x",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.1",
    "typescript": "^4.0.0"
  },
  "main": "dist/src/functions/*.js"
}
